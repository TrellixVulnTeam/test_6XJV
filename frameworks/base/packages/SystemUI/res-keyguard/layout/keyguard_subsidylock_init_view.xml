<?xml version="1.0" encoding="utf-8"?>
<com.android.keyguard.KeyguardSubsidyLockInitView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:androidprv="http://schemas.android.com/apk/res-auto"
    android:id="@+id/keyguard_subsidylock_init_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="320dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        android:background="#FFFFFF"
        android:orientation="vertical"
        android:layout_marginTop="245dp" >

        <TextView
            android:id="@+id/init_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="25dp"
            android:text="@string/init_title"
            android:textColor="#333333"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/init_message"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="25dp"
            android:text="@string/init_message"
            android:textColor="#009385"
            android:textSize="16sp" />
    </LinearLayout>

</com.android.keyguard.KeyguardSubsidyLockInitView>